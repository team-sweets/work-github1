<div class="row">
	<div class="col-lg-8">
	</div>
</div>


<h4><strong>商品一覧</strong><span>（全x件）</span></h4>
<% @products.each do |product| %>
<div>
  <%= attachment_image_tag product, :image ,size: "300x200" %>

  <p><strong>商品名</strong> <%= product.name %></p>

  <p>¥<%= product.tax_out_price.to_s(:delimited) %></p>
</div>
<% end %>